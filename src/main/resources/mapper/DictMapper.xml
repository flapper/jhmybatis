<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.codezen.leaves.s001.DictService" >
    
    <sql id="Base_Column_List" >
         DIC_TYPE
        ,DIC_KEY
        ,DIC_ENG
        ,DIC_KOR
        ,SYNONYM_ENG
        ,SYNONYM_KOR
        ,STD_CAT_NM
        ,STD_CAT_KEY
        ,USE_YN
        ,MEMO
        ,INS_USER_CD
        ,UPT_DT
        ,INS_DT
        ,UPT_USER_CD
    </sql>

    <select id="getAll" parameterType="HashMap" resultType="HashMap" >
       SELECT 
       <include refid="Base_Column_List" />
	   FROM tb_dictionary_info
        LIMIT 200
    </select>

    <select id="getOne" parameterType="HashMap" resultType="HashMap" >
        SELECT 
       <include refid="Base_Column_List" />
	   FROM tb_dictionary_info
	   WHERE dic_key = #{id}
    </select>

</mapper>